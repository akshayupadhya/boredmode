{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended",
		"tslint-config-standard",
		"tslint-config-prettier",
		"tslint-react"
	],
	"jsRules": {},
	"rules": {
		"no-angle-bracket-type-assertion": false,
		"ter-indent": [true, "tab"],
		"indent": [true, "tabs", 2],
		"ordered-imports": false,
		"object-literal-sort-keys": false,
		"member-ordering": false,
		"jsx-no-lamda": false,
		"jsx-boolean-values": false
	},
	"rulesDirectory": []
}
