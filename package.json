{
	"name": "boredmode",
	"version": "0.0.1",
	"main": "src/app.ts",
	"repository": "git@github.com:akshayupadhya/boredmode.git",
	"author": "akshay upadhya <akshaysrinagar@gmail.com>",
	"private": false,
	"scripts": {
		"start": "node dist/Main.js",
		"build-backend": "tsc --build tsconfig.json",
		"build-frontend": "parcel build src/frontend/Main.tsx -d dist/public -o bundle.js",
		"watch-backend": "nodemon",
		"watch-frontend": "parcel watch src/frontend/Main.tsx -d dist/public -o bundle.js",
		"watch": "concurrently \"npm:watch-*\"",
		"build": "concurrently \"npm:build-*\"",
		"frontend": "parcel src/frontend/main.tsx",
		"postinstall": "ts-node src/build.ts",
		"lint": "tslint 'src/**/*.{ts,tsx}'"
	},
	"devDependencies": {
		"@types/dotenv": "^6.1.0",
		"@types/express": "^4.16.0",
		"@types/express-graphql": "^0.6.2",
		"@types/node": "^10.12.10",
		"@types/react": "^16.7.7",
		"@types/react-dom": "^16.0.10",
		"@types/react-router-dom": "^4.3.1",
		"@types/serialize-javascript": "^1.5.0",
		"concurrently": "^4.1.0",
		"nodemon": "^1.18.6",
		"parcel-bundler": "^1.10.3",
		"prettier": "^1.15.3",
		"ts-node": "^7.0.1",
		"tslint": "^5.12.0",
		"tslint-config-prettier": "^1.17.0",
		"tslint-config-standard": "^8.0.1",
		"tslint-react": "^3.6.0",
		"typescript": "^3.1.6"
	},
	"dependencies": {
		"dotenv": "^6.1.0",
		"express": "^4.16.4",
		"express-graphql": "^0.7.1",
		"graphql": "^14.0.2",
		"isomorphic-fetch": "^2.2.1",
		"react": "^16.6.3",
		"react-dom": "^16.6.3",
		"react-router-dom": "^4.3.1",
		"serialize-javascript": "^1.5.0"
	}
}
